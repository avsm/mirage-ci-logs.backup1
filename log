RUN git fetch origin pull/551/head:cibranch
RUN git checkout cibranch
RUN cat /etc/os-release || true
RUN opam pin add -n mirage /home/opam/src
RUN opam depext -uy mirage
RUN opam install -j 2 -vy mirage[2016-08-18 10:57.12] Success
